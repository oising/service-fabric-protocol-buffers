<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" 
    xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <GrpcToolOs Condition=" '$(TargetPlatformIdentifier)' == 'Windows' ">windows</GrpcToolOs>
        <GrpcToolArch Condition=" '$(TargetSDKArchitecture)' == 'amd64'">x64</GrpcToolArch>
        <GrpcToolOs Condition=" '$(GrpcToolOs)' == '' ">windows</GrpcToolOs>
        <GrpcToolArch Condition=" '$(GrpcToolArch)' == ''">x86</GrpcToolArch>
        <_protoc_common_props_imported>true</_protoc_common_props_imported>
    </PropertyGroup>
</Project>
